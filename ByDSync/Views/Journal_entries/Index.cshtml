@model IEnumerable<ByDSync.Schema.journal_entries>

@{
    ViewBag.Title = "Index";
}

<h2>Journal Entries</h2>

<p>
    @Html.ActionLink("Sync Data", "Synchronize")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.jour_number)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.gl_act)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.gl_acct_type)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.created_date)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.changed_date)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.jour_id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.debit_credit)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.fiscal_year)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.created_by)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.changed_by)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.comp_cur_amt)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.item_cur_amt)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.tran_cur_amt)
        </th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.jour_number)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.gl_act)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.gl_acct_type)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.created_date)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.changed_date)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.jour_id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.debit_credit)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.fiscal_year)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.created_by)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.changed_by)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.comp_cur_amt)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.item_cur_amt)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.tran_cur_amt)
        </td>
    </tr>
}

</table>
